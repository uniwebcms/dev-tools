{
  "name": "@uniwebcms/dev-tools",
  "version": "1.0.3",
  "description": "Uniweb CLI for managing sites, modules and components",
  "type": "module",
  "main": "./src/index.js",
  "bin": {
    "extract-tools": "./bin/extract-tools.js"
  },
  "scripts": {
    "prepare": "node ./bin/extract-tools.js",
    "build": "node ./bin/extract-tools.js",
    "start": "node ./bin/extract-tools.js",
    "dev": "npm run build && node ./bin/extract-tools.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "cli",
    "website",
    "ai tools",
    "uniweb"
  ],
  "author": "Proximify Inc",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "typescript": "^5.8.2"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  }
}
